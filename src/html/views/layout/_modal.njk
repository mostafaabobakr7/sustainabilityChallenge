<!-- Modal log off -->
<div class="modal fade" id="logoffModal">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header bg-danger text-white">
        <h6 class="modal-title" id="logoffModalLabel">LOG OFF</h6>
        <button type="button" class="close modalClose">
          <span>&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>ARE YOU SURE ?</p>
      </div>
      <div class="modal-footer d-flex justify-content-between">
        <button type="button" class="modalClose btn btn-lg btn-success px-4">Stay Online</button>
        <a href="./login.html" class="btn btn-lg btn-secondary px-5">Log Off</a>
      </div>
    </div>
  </div>
</div>
<!-- Modal delete -->
<div class="modal fade" id="deleteModal">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header bg-danger text-white">
        <h6 class="modal-title" id="deleteModalLabel">Delete</h6>
        <button type="button" class="close modalClose">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p>ARE YOU SURE ?</p>
      </div>
      <div class="modal-footer d-flex justify-content-between">
        <button type="button" class="modalClose btn btn-lg btn-success px-4">NO</button>
        <button type="button" class="btn btn-lg btn-secondary px-5">YES</button>
      </div>
    </div>
  </div>
</div>
